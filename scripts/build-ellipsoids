# $Id: build-ellipsoids,v 1.1 2002/06/11 19:19:37 cepek Exp $


if pwd | grep /script$; then cd ..; fi

echo
echo "Building ellipsoids"
echo

(cd scripts ; make -f Makefile-ellipsoids )

scripts/ellipsoids_xml xml/ellipsoids.xml gamalib/

rm -f xml/ellipsoids.html
mv gamalib/ellipsoids.html xml

