#!/bin/sh

set -e

RES=@GAMA_RESULTS@/gama-local-adjustment
if [ ! -d $RES ]; then
    mkdir -p $RES
fi;

for g in @INPUT_FILES@
do
for a in svd gso cholesky envelope
do
    echo  @PACKAGE_VERSION@ $g $a
    @top_builddir@/bin/gama-local @GAMA_INPUT@/$g.gkf \
	--algorithm $a \
	--text $RES/$g-$a.txt \
	--html $RES/$g-$a.html \
	--xml  $RES/$g-$a.xml \
	--svg  $RES/$g-$a.svg
done
done
