CXXFLAGS += -I../.. 

all: gamalib demo-dataparser

gamalib: 
	(cd ../..; make gamalib)

demo-dataparser : dataparser.o ../../gamaprog/linux/lib/gamalib.a
	$(CXX) $(CXXFLAGS) -DGNU_Gama_DataParser_demo -o demo-dataparser \
		dataparser.o ../../gamaprog/linux/lib/gamalib.a

dataparser.o : dataparser.h dataparser.cpp Makefile-dataparser-demo
	$(CXX) $(CXXFLAGS) -DGNU_Gama_DataParser_demo -c dataparser.cpp


clean:
	rm -f *.o *~ demo*




