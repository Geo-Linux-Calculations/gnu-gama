<!ELEMENT  gama-xml     (network) >
<!ELEMENT  network      (description | parameters | points-observations)* >
<!ELEMENT  description  (#PCDATA) >
<!ELEMENT  parameters   EMPTY >
<!ELEMENT  points-observations  (point | obs | coordinates)* >

<!ELEMENT  point  EMPTY >

<!ELEMENT  obs  (direction | distance | angle)* 
                cov-mat? 
                (direction | distance | angle)* >
<!ELEMENT  cov-mat    (NMTOKEN)+ >
<!ELEMENT  direction  EMPTY >
<!ELEMENT  distance   EMPTY >
<!ELEMENT  angle      EMPTY >

<!-- observed coordinates -->
<!ELEMENT coordinates (point)* cov-mat? (point)* >

<!ATTLIST parameters 

   txt        CDATA    #IMPLIED
   stx        CDATA    #IMPLIED
   opr        CDATA    #IMPLIED
   sigma-apr  NMTOKEN  "10.0" 
   conf-pr    NMTOKEN  "0.95"
   tol-abs    NMTOKEN  "1000.0"
   sigma-act           (aposteriori|apriori) "apriori" >

<!ATTLIST points-observations

   distance-stdev   NMTOKEN  #IMPLIED
   direction-stdev  NMTOKEN  #IMPLIED
   angle-stdev      NMTOKEN  #IMPLIED  >

<!ATTLIST point

   id  CDATA    #REQUIRED
   y   NMTOKEN  #IMPLIED
   x   NMTOKEN  #IMPLIED
   v   NMTOKEN  #IMPLIED
   xy  (fixed | free | constrained | unused) "unused" >

<!ATTLIST obs    
   at           CDATA   #REQUIRED
   orientation  NMTOKEN #IMPLIED  >

<!ATTLIST cov-mat
   dim   NMTOKEN  #REQUIRED
   band  NMTOKEN  #REQUIRED >

<!ATTLIST direction 

   to     CDATA    #REQUIRED
   val    NMTOKEN  #REQUIRED
   stdev  NMTOKEN  #IMPLIED  >

<!ATTLIST distance 

   at     CDATA    #IMPLIED
   to     CDATA    #REQUIRED
   val    NMTOKEN  #REQUIRED
   stdev  NMTOKEN  #IMPLIED  >

<!ATTLIST angle 

   at     CDATA    #IMPLIED
   to     CDATA    #REQUIRED
   rs     CDATA    #REQUIRED
   val    NMTOKEN  #REQUIRED
   stdev  NMTOKEN  #IMPLIED  >
















