<!ELEMENT gama-xml (sit) >
<!ELEMENT sit   (popis | parametry | body-mereni)* >
<!ELEMENT popis (#PCDATA) >
<!ELEMENT parametry EMPTY >
<!ELEMENT body-mereni (bod | osnova | delka | uhel)* >
<!ELEMENT bod EMPTY >
<!ELEMENT osnova (smer | delka | uhel)* >
<!ELEMENT smer  EMPTY >
<!ELEMENT delka EMPTY >
<!ELEMENT uhel  EMPTY >

<!ATTLIST parametry 

   txt     CDATA   #IMPLIED
   stx     CDATA   #IMPLIED
   opr     CDATA   #IMPLIED
   m0-apr  NMTOKEN "10.0" 
   konf-pr NMTOKEN "0.95"
   tol-abs NMTOKEN "1000.0"
   typ-m0  (empiricka|apriorni) "empiricka" >

<!ATTLIST body-mereni

   delka-str     NMTOKEN #IMPLIED
   delka-str-km  NMTOKEN #IMPLIED 
   delka-str-exp NMTOKEN #IMPLIED
   smer-str      NMTOKEN #IMPLIED
   uhel-str      NMTOKEN #IMPLIED  >

<!ATTLIST bod

   cislo CDATA   #REQUIRED
   y     NMTOKEN #IMPLIED
   x     NMTOKEN #IMPLIED
   v     NMTOKEN #IMPLIED
   typ   (pevny | urcovany | operny | nepouzit) "nepouzit" >

<!ATTLIST osnova    
   stan     CDATA   #REQUIRED
   or-posun NMTOKEN #IMPLIED  >

<!ATTLIST smer 

   cil CDATA   #REQUIRED
   mer NMTOKEN #REQUIRED
   str NMTOKEN #IMPLIED  >

<!ATTLIST delka 

   stan CDATA   #IMPLIED
   cil  CDATA   #REQUIRED
   mer  NMTOKEN #REQUIRED
   str  NMTOKEN #IMPLIED  >

<!ATTLIST uhel 

   stan  CDATA   #IMPLIED
   cil-l CDATA   #REQUIRED
   cil-p CDATA   #REQUIRED
   mer   NMTOKEN #REQUIRED
   str   NMTOKEN #IMPLIED  >
















